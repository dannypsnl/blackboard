   },
   "outputs": [],
   "source": [
    "F1 = (r * cos(y) + a) * cos(x)\n",
    "F2 = (r * cos(y) + a) * sin(x)\n",
    "F3 = r * sin(y) * cos(x/2)\n",
    "F4 = r * sin(y) * sin(x/2)"
   ]
  },
